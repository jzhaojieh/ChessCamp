      <%= render :partial => "camp_instructors_p_vue"%>
    <% end %>
    <%# render :partial => "camp_students"%>
  </div>
</div>
<div class="row">
  <div class="col s12">
    <%= render :partial => "partials/object_options", locals: {model_name: "camp", object: @camp} %>
  </div>
</div>